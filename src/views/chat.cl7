<div class="chat-container">
  <header class="chat-header">
    <h1>
      <i class="fas fa-comments"></i>  
       WebChat
    </h1>
    <a href="/" class="btn">Leave Room</a>
  </header>
  <main class="chat-main">
    <div class="chat-sidebar">
      <h3><i class="fas fa-home"></i> Room Name:</h3>
      <h2 id="room-name"></h2>
      <h3 ><i class="fas fa-users"></i> Users 
        <span id="countUsers"></span>
      </h3>
      <p style="height:70px;">Gi√° coin : <span id="coin"></span></p>
      <ul id="users"></ul>
    </div>
    <div class="chat-messages">
       <div class="message-welcome">
          <span style="text-align: center;" class="welcome-webchat">Loading...</span>
        </div>

        <div class="message">
        </div>
        <div class="auto-message">
          
        </div>
    </div>
  </main>
   <div class="chat-form-container">
    <form action="/webhook" method="POST" id="chat-form">
      <input id="text" type="text" placeholder="Enter Message" required />
      <button type="submit" class="btn" id="sendButton"><i class="fas fa-paper-plane"></i> Send</button>
   
    </form>
  </div>
</div>
